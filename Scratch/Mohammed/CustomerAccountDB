CREATE TABLE `CustomerAccount` (
	`Forename`	TEXT,
	`Surname`	TEXT,
	`Address`	TEXT,
	`Postcode`	TEXT,
	`Phone`	TEXT,
	`Email address`	TEXT,
	`CustomerID`	TEXT,
	`CustomerType`	TEXT,
	`NumberOfBookings`	INTEGER,
	`NumberOfBills`	INTEGER,
	`TotalCost`	INTEGER,
	`WarrantyStatus`	BOOLEAN,
	PRIMARY KEY(`CustomerID`)
);
